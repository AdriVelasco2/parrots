    font-family: "Cinzel";
   
    font-weight: 700;
    font-style: normal;
  }
  .date-picker-container label {
    color: white;
  }
  .date-picker-container label,
.date-picker-container label:focus,
.date-picker-container label:active,
.date-picker-container label[data-shrink="true"] {
  color: #e3d779 !important;
}
.date-picker-container input {
  color: #FFFFFF !important;               /* Fuerza el color blanco en el texto */
}

.date-picker-container input::placeholder {
  color: #FFFFFF !important;               /* Fuerza el color blanco en el placeholder */
}
.data-\[editable\=true\]\:text-foreground[data-editable=true] {
    --tw-text-opacity: 1;
    color: #ffffff;
}

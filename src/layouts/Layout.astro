    --tw-text-opacity: 1;
    color: #e3d779;
}
.text-foreground {
    --tw-text-opacity: 1;
    color: #d6c967;
}
.data-\[disabled\=true\]\:text-default-300[data-disabled=true] {
    --tw-text-opacity: 1;
    color: #e3d779 /;
}
.bg-default-50 { 
    --tw-bg-opacity: 1;
    background-color: rgb(3, 3, 3);
}
.bg-content1 {
    --tw-bg-opacity: 1;
    background-color: #e9da66;
}

.after\:bg-primary::after {
    content: var(--tw-content);
    --tw-bg-opacity: 1;
    background-color: rgb(207, 181, 66);
}
.text-secondary {
    --tw-text-opacity: 1;
    color:#e3d779;
}
.after\:bg-secondary::after{
	content: var(--tw-content);
    --tw-bg-opacity: 1;
    background-color: rgb(207, 181, 66);
}
.group[data-has-value=true] .group-data-\[has-value\=true\]\:text-default-foreground {
    --tw-text-opacity: 1;
    color: #FFFFFF !important;
}
input:-webkit-autofill,
input:-webkit-autofill:hover,
input:-webkit-autofill:focus {
  -webkit-text-fill-color: white; /* Cambia el color del texto a blanco */
  -webkit-box-shadow: 0 0 0px 1000px #333 inset; /* Cambia el fondo, reemplaza #333 por el color deseado */
  box-shadow: 0 0 0px 1000px #333 inset;
  transition: background-color 5000s ease-in-out 0s;
}
.text-medium {
    font-size: 12px;
    line-height: var(--nextui-line-height-medium);
}
.bg-default-100 {
    --tw-bg-opacity: 1;
    background-color: rgba(181, 138, 45, 1);
}
